pp/src/core/Home.js",["63"],"import React, { useState, useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\n\nimport Base from \"./Base\";\nimport { getDetails } from \"./helper/coreapicalls\";\nimport Card from \"./Card\";\n\nconst Home = () => {\n  const [details, setDetails] = useState([]);\n  const [error, setError] = useState(false);\n\n  const loadAllDetails = () => {\n    getDetails()\n      .then((data) => {\n        if (data.error) {\n          setError(true);\n        } else {\n          data.reverse();\n          setDetails(data);\n          setError(false);\n        }\n      })\n      .catch((err) => {\n        setError(true);\n      });\n  };\n\n  useEffect(() => {\n    setInterval(loadAllDetails, 1000);\n    // loadAllDetails();\n  }, []);\n\n  const loadContent = () => {\n    return (\n      <div className=\"container\">\n        <div className=\"row\">\n          {details.map((detail, index) => {\n            return (\n              <div key={detail.id} className=\"mx-auto\">\n                <Card detail={detail} />\n              </div>\n            );\n          })}\n        </div>\n      </div>\n    );\n  };\n\n  const showError = () => {\n    return (\n      <div className=\"text-center\">\n        <br />\n        <br />\n        <br />\n        <br />\n        <h4 className=\"text-danger\">\n          <span className=\"align-middle\">Something went wrong!</span>\n        </h4>\n      </div>\n    );\n  };\n\n  return (\n    <Base>\n      <br />\n      <br />\n      <br />\n      <h1 className=\"text-center text-success\">Home Component</h1>\n      <br />\n      {error ? showError() : loadContent()}\n      <br />\n      <br />\n      <br />\n      <br />\n    </Base>\n  );\n};\n\nexport default Home;\n","/home/shouvik/Desktop/testReactProject/my-app/src/core/helper/coreapicalls.js",[],"/home/shouvik/Desktop/testReactProject/my-app/src/Backend.js",[],"/home/shouvik/Desktop/testReactProject/my-app/src/core/Card.js",[],"/home/sho